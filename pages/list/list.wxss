/* pages/list/list.wxss */
.list-container {
  height: 100%;
  background-color: #f0f0f0;
}
.info-card {
  width: 96%;
  height: 91px;
  margin: 6px auto 6px;
  background-color: #fff;
  border-radius: 1px;
  box-shadow: 0 1px 2px 0 rgba(0,0,0,.1);
}
.start-icon {
  display: inline-block;
  margin-top: 12px;
  margin-left: 10px;
  margin-right: 11px;
  width: 18px;
  height: 17px;
  color: #ffffff;
  font-size: 12px;
  vertical-align: center;
  text-align: center;
  background-color: rgb(52,115,243);
}
.end-icon {
  display: inline-block;
  margin-left: 10px;
  margin-right: 11px;
  width: 18px;
  height: 17px;
  color: #ffffff;
  font-size: 12px;
  text-align: center;
  vertical-align: center;
  background-color: rgb(245,141,79);
}
.location-text {
  display: inline-block;
  font-size: 13px;
}
.info-text {
  display: inline-block;
  font-size: 12px;
  color: rgb(114,114,114);
}
.ml10 {
  margin-left: 10px;
}
.user-icon {
  display: inline-block;
  font-size: 12px;
  margin-left: 10px;
  margin-right: 11px;
  color: rgb(114,114,114);
}